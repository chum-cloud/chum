# CHUM Cloud Agent Configuration
# Copy to .env and fill in values

# Helius RPC endpoint (mainnet)
RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY

# Agent signing keys (base58 encoded private keys)
# Generate with: solana-keygen new --no-bip39-passphrase
WHALE_AGENT_KEY=
VOLUME_AGENT_KEY=
MOMENTUM_AGENT_KEY=

# Alpha Room address (on-chain coordination)
CHUM_ROOM=chumAA7QjpFzpEtZ2XezM8onHrt8of4w35p3VMS4C6T

# Agent behavior
POLL_INTERVAL=30
WHALE_MIN_SOL=500
VOLUME_SPIKE_MULTIPLIER=3
MOMENTUM_LOOKBACK_MINUTES=15
